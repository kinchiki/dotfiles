[user]
    name = Akito Naito
    email = naito.akito@gmail.com
[core]
    excludesfile = ~/.gitignore_global
    editor = vim -c \"set fenc=utf-8\"
    precomposeunicode = true
    quotepath = false
[color]
    diff   = auto
    status = auto
    branch = auto
    ui     = auto
[alias]
    addu = add -u
    adda = add -A
    ci  = commit
    cim = commit -m
    cia = commit --amend
    co  = checkout
    coma = checkout master
    cp  = cherry-pick
    st  = status -sb
    br  = branch
    l   = log --oneline
    lg  = log --graph --decorate --oneline
    dcw = diff --color-words
    dc  = diff --cached
    df  = diff
    dw  = diff -w
    g   = grep
    gi  = grep -i
    gc  = grep -C
    gci = grep -i -C
    si  = switch
    sta = stash
    stal = stash list
    stalp = stash list -p
    pl = pull
    rc = rebase --continue
[grep]
    lineNumber = true
[push]
    default = simple
[ghq]
    root = ~/src
[commit]
    template = ~/.stCommitMsg
[credential]
	helper = !aws codecommit --profile dev credential-helper $@
	helper =
	helper = /usr/local/share/gcm-core/git-credential-manager
#	UseHttpPath = true
[merge]
	ff = false
[pull]
	ff = only
	rebase = merges
[credential "https://dev.azure.com"]
	useHttpPath = true
[include]
    path = .gitconfig.local
